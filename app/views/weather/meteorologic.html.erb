<div class="warp">
	<div class="sef-container">
		<div class="tab-title col-xs-12 text-center">
			<div class="col-xs-3">数据名称</div>
			<div class="col-xs-3">采集周期</div>
			<div class="col-xs-3">上次采集</div>
			<div class="col-xs-3">状态</div>
		</div>
		<div class="tab-line text-center clearfix">
		<% 6.times do |i| %>
			<div class="col-xs-12 active-line">
				<div class="col-xs-3">社区预警</div>
				<div class="col-xs-3">5min</div>
				<div class="col-xs-3">5min前</div>
				<div class="col-xs-3"><i class="fa fa-circle fa-red"></i>延迟</div>
			</div>
			<hr class="line-hr">
		<% end %>
		<% 10.times do |i| %>
			<div class="col-xs-12 active-line">
				<div class="col-xs-3">社区预警</div>
				<div class="col-xs-3">15min</div>
				<div class="col-xs-3">8min前</div>
				<div class="col-xs-3"><i class="fa fa-circle fa-green"></i>正常</div>
			</div>
			<hr class="line-hr">
		<% end %>
			<div class="col-xs-12 active-line">
				<div class="col-xs-3">社区预警</div>
				<div class="col-xs-3">5min</div>
				<div class="col-xs-3">5min前</div>
				<div class="col-xs-3"><i class="fa fa-circle fa-red"></i>延迟</div>
			</div>
		</div>
	</div>
</div>