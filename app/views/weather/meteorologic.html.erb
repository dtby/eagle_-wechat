<div class="warp">
	<div class="sef-container">
		<div class="tab-title col-xs-12 text-center">
			<div class="col-xs-3">数据名称</div>
			<div class="col-xs-3">采集周期</div>
			<div class="col-xs-3">上次采集</div>
			<div class="col-xs-3">状态</div>
		</div>
		<div class="tab-line text-center clearfix">
		<% @task_logs.each_with_index do |task_log, index| %>
			<%= link_to "#" do %>
			<div class="col-xs-12 active-line">
				<div class="col-xs-3"><%=  task_log["identifier"] %></div>
				<div class="col-xs-3" style="line-height: 40px;"><%= time_mini(task_log["start_time"], task_log["end_time"]) %>min</div>
				<div class="col-xs-3" style="line-height: 40px;"><%= (index == 0) ? 0 : (@tasks_logs[index]["start_time"].to_i -  @tasks_logs[index - 1 ]["end_time"].to_i)/6000 %>min前</div>
				<div class="col-xs-3" style="line-height: 40px;"><i class="fa fa-circle fa-red"></i>延迟</div>
			</div>
			<hr class="line-hr">
			<% end %>
		<% end %>
			<!-- <div class="col-xs-12 active-line">
				<div class="col-xs-3">健康气象<br>(COPD、儿童)</div>
				<div class="col-xs-3" style="line-height: 40px;">5min</div>
				<div class="col-xs-3" style="line-height: 40px;">5min前</div>
				<div class="col-xs-3" style="line-height: 40px;"><i class="fa fa-circle fa-red"></i>延迟</div>
			</div> -->
		</div>
	</div>
</div>