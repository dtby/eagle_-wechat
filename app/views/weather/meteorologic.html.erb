<div class="warp">
	<div class="sef-container">
		<div class="tab-title col-xs-12 text-center">
			<div class="col-xs-3">数据名称</div>
			<div class="col-xs-3">采集周期</div>
			<div class="col-xs-3">上次采集</div>
			<div class="col-xs-3">状态</div>
		</div>
		<div class="tab-line text-center clearfix">
			<% @task_logs.each_with_index do |task_log, index| %>
				<%= link_to "#" do %>
				<div class="col-xs-12 active-line">
					<div class="col-xs-3"><%=  task_log.try(:task_name) %></div>
					<div class="col-xs-3" style="line-height: 40px;"><%= task_log.end_time - task_log.start_time %>s</div>
					<div class="col-xs-3" style="line-height: 40px;">
						<%= pre_time(task_log, @task_logs).to_s %>min前
					</div>
					<div class="col-xs-3" style="line-height: 40px;">
					<% if pre_time(task_log, @task_logs) > 300 %>
						<i class="fa fa-circle fa-red"></i>延迟
					<% else %>
						<i class="fa fa-circle fa-green"></i>正常
						<% end %>
					</div>
				</div>
				<hr class="line-hr">
				<% end %>
			<% end %>
		</div>
	</div>
</div>