<div class="warp">
	<div class="sef-container">
		<div class="tab-title col-xs-12 server-tab">
			<div class="col-xs-4 server-xs4">服务器编码</div>
			<div class="col-xs-2">CPU</div>
			<div class="col-xs-2">内存</div>
			<div class="col-xs-2">网络</div>
			<div class="col-xs-2 text-center">时延</div>
		</div>
		<div class="tab-line text-center clearfix">
			<% @infos.each do |info| %>
				<%= link_to detail_machine_path(params[:id]) do |i| %>
				<div class="col-xs-12 server-line active-line">
					<div class="col-xs-4"><%= @machine.identifier %></div>
					<div class="col-xs-2"><p>
						<i class="fa fa-circle fa-red"></i>75%</p>
						<p class="server-number"><%= info[:cpu][:real] %>/<%= info[:cpu][:total] %></p>
					</div>
					<div class="col-xs-2">
						<p><i class="fa fa-circle fa-green"></i>20%</p>
						<p class="server-number"><%= info[:memory][:swap_total] %>/<%= info[:memory][:total] %></p>
					</div>
					<div class="col-xs-2"><i class="fa fa-circle fa-green"></i>正常</div>
					<div class="col-xs-2"><i class="fa fa-circle fa-green"></i>20ms</div>
				</div>
				<% end %>
				<hr class="line-hr">
			<% end %>
		</div>
	</div>
</div>